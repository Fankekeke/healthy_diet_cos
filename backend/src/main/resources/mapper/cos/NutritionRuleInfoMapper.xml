<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cc.mrbird.febs.cos.dao.NutritionRuleInfoMapper">

    <!-- 分页查询营养规则信息 -->
    <select id="queryNutritionRulePage" resultType="java.util.LinkedHashMap">
        select
            id,
            heat,
            protein,
            fat
        from
            nutrition_rule_info
        where 1 = 1
    </select>
</mapper>
