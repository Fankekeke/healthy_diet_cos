page {
    padding-top: 50px;
    padding-bottom:20rpx;
    background-color: white;
}
.head-title{
    margin: 0 auto;
}
.cu-custom .cu-bar{
    padding-right:inherit;
    /* background-image:linear-gradient(45deg, #31c4ae, #086b5c); */
    background-image:linear-gradient(45deg, #31c4ae, #086b5c);
}
.bg-gradual-green {
    background-image:linear-gradient(45deg, #31c4ae, #086b5c);
}
.text-green, .line-green, .lines-green {
    color:#e45c5c;
}
.swiperitem{
    background: #fff;
    height:55px;
}
.canui-video{
    width: 100%;
    height: 425rpx;
}
.nav-list,.nav-li{
    padding: 0px;
    z-index: 0;
}
.canui-yx-url{
    width:46% !important;
    margin:0 2% 30rpx !important;
}
.canui-flex-car{
    padding-top: 10px;
}
.canui-flex-car .cu-card>.cu-item {
    margin: 0px;
    position: relative;
}
.canui-flex-car .cu-list.menu-avatar>.cu-item {
    padding-left:15rpx;
    height:100rpx;
}
.canui-flex-car .cu-card .cu-avatar{
    position:absolute;
    right:10px;
    bottom:72rpx;
}
.canui-flex-car .shadow-warp {
    box-shadow:0 0 30rpx rgba(0, 0, 0, 0.1);
}
.canui-flex-car .cu-list.menu>.cu-item .content {
    font-size:26rpx;
}
.canui-flex-car .text-sm {
    font-size:20rpx;
}
.canui-flex-car .text-grey {
    color: #000;
}
.canui-flex-car image{
    height: 90px !important;
}
.canui-gps .justify-between{
    padding: 0 10px;
}

.content .text-sm{
    overflow:hidden;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:1;
}
.content .text-titile{
    font-size: 13px;
    overflow:hidden;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:1;
}
.canui-fixed{
    position: fixed;
    bottom: 0px;
    width: 100%;
    z-index:999999;
}

.canui-card-order{
    padding:0 20rpx;
}
.canui-card-order .canui-order-item{
    position: relative;
    margin-bottom:30rpx;
    padding:20rpx;
}
.cu-list.menu-avatar>.cu-item {
    padding-left: 30rpx;
    height: 140rpx;
  }

.head-title-box .icon-shop{
    margin-right:10rpx;
}
.head-title-box .icon-right{
    margin-left:5rpx;
}
.canui-order-item .cu-list.menu-avatar>.cu-item{
    padding:0;
    /* padding-left:110rpx; */
}
.canui-order-item .cu-list.menu-avatar>.cu-item>.cu-avatar{
    left:0rpx;
}
.canui-order-item .cu-list .cu-item .canui-xzwz{
    -webkit-line-clamp:2;
}
.canui-order-item .cu-list.menu>.cu-item .content{
    font-size:27rpx;
    margin-right:40rpx;
}
.canui-order-item .cu-list.menu-avatar>.cu-item .action {
    text-align:right;
    margin-top:-10rpx;
}
.canui-order-item .cu-list.menu>.cu-item .action .text-price{
    font-size:28rpx;
}
.canui-order-item .order-price-box{
    text-align:right;
    font-size:26rpx;
}
.canui-order-item .order-btn-box .margin-tb-sm{
    margin-bottom:0;
}
.canui-order-item .order-btn-box .margin-tb-sm .cu-btn{
    font-size:24rpx;
}
.canui-order-item .order-btn-box .margin-tb-sm .cu-btn[class*="line"]::after {
    /*按钮边框大小*/
    border: 3rpx solid currentColor;
}
.canui-order-item .order-btn-box .margin-tb-sm .line-gray{
    color: #666;
}
.canui-order-item .order-btn-box .margin-tb-sm + .margin-tb-sm{
    margin-left: 10rpx;
}

.cu-custom{
    position: absolute;
    top: 0px;
    width: 100%;
}
.cu-custom .cu-bar{
    padding-right:inherit;
    background-image:linear-gradient(45deg, #31c4ae, #086b5c);
}
.box{
    position: absolute;
    top: 64px;
    width: 100%;
}

.canui-card{
    padding:0 20rpx;
}
.canui-card .canui-item{
    margin-bottom:30rpx;
}
.canui-card-bg-img {
    font-variant:small-caps;
    margin:0;
    padding:0;
    display:inline-flex;
    text-align:center;
    justify-content:center;
    align-items:center;
    background:#ccc;
    color:#fff;
    white-space:nowrap;
    position:relative;
    width:100%;
    height:180rpx;
    background-size:cover;
    background-position:center;
    vertical-align:middle;
    font-size:26rpx;
}
.bg-shadeTop {
    background-image: linear-gradient(rgba(0, 0, 0, 0.3), rgba(0, 0, 0, 0));
    color:#fff;
    width:100%;
    height:100%;
}
.canui-card-head{
    text-align:left;
    padding:20rpx;
    position: relative;
}
.canui-card-head .cu-avatar {
    border:1rpx solid #fff;
}
.canui-card-head .canui-card-name{
    position: relative;
    margin-left:20rpx;
    font-size:30rpx;
    top:5rpx;
}

.--van-button-large-height {
    height: 100px;
}